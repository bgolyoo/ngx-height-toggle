{
  "name": "ngx-height-toggle",
  "version": "1.0.4",
  "description": "Angular(2+) directive to change the height of html elements to their content's height on toggle and on content change.",
  "keywords": [
    "angular",
    "angular2",
    "ngx",
    "ngx-height-toggle",
    "height-toggle",
    "content-change",
    "npm-package"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bgolyoo/ngx-height-toggle.git"
  },
  "author": "Bodor Gergely",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bgolyoo/ngx-height-toggle/issues"
  },
  "homepage": "https://github.com/bgolyoo/ngx-height-toggle#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "build": "ng build",
    "build:lib": "ng test ngx-height-toggle --watch=false && ng build ngx-height-toggle --prod",
    "build:demo": "ng test ngx-height-toggle-app --watch=false && ng build --prod --build-optimizer --base-href \"https://bgolyoo.github.io/ngx-height-toggle/\"",
    "deploy:demo": "angular-cli-ghpages --dir \"dist/ngx-height-toggle-app\" --no-silent",
    "test:lib": "ng test ngx-height-toggle",
    "test:demo": "ng test ngx-height-toggle-app",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@ngx-prism/core": "^2.0.0",
    "@types/prismjs": "1.9.0",
    "bootstrap": "^4.1.1",
    "core-js": "^2.5.4",
    "lodash-es": "^4.17.10",
    "prismjs": "1.9.0",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.6",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "@angular/cli": "~6.0.7",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0-rc.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}